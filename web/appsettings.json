{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "WebTestService": {
    "VideoStore": "VideoStore",
    "ScreenHeight": 768,
    "ScreenWidth": 1366,
    "Headless": true,
    "SupportedAction": [
      "OpenPage",
      "OpenPageFreeSize",
      "WaitElement",
      "Input",
      "ClickAndWait",
      "Delay",
      "CheckNotEqual",
      "CheckEqual",
      "TakeScreenshot",
      "SendEmailReport",
      "PressKey",
      "CheckNotEqual",
      "CheckEqual",
      "RunSuiteCheckElement",
      "ReopenBroswer",
      "RunSuite",
      "RunScript",
      "OpenPageWithRecord"
    ]
  },
  "SignalRService": {
    "Endpoint": "https://loadcontrol.pharmacity.store/DataHub",
    "UseHttp": false,
    "StartWait": 100,
    "Capacity": "Web",
    "Device": "Chrome",
    "MaxSession": 50
  },
  "InfluxDBService": {
    "Enabled": false,
    "Endpoint": "https://loadmaster.pharmacity.store",
    "Token": "KCxRDpRueXsMBv5KIvUPA3_vmu05iNzCMgZSeSwdjQ3MdhVzApYu7fE4a_-f5i5KUCET4bVrwGfRiYOdTXryrw==",
    "Organization": "pharmacity",
    "BucketID": "automation",
    "WriteInterval": 5000,
    "MaxPointToWrite":  100
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({ThreadId}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Debug_Log/WebAuto-Full.log",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({ThreadId}) {Message}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 104857600, //1024 * 1024 * 100 --> 100MB per file
          "retainedFileCountLimit": 10, // Keep max 10 file --> 1GB of log storage
          "rollOnFileSizeLimit": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Debug_Log/WebAuto-Warning.log",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({ThreadId}) {Message}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Warning",
          "fileSizeLimitBytes": 104857600, //1024 * 1024 * 100 --> 100MB per file
          "retainedFileCountLimit": 10, // Keep max 10 file --> 1GB of log storage
          "rollOnFileSizeLimit": true
        }
      }
    ]
  }
}
